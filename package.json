{
  "name": "ragussy",
  "version": "1.0.0",
  "description": "Universal RAG chatbot with full management UI",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "setup": "tsx src/cli/setup.ts",
    "dev": "tsx watch src/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "tsc && cd frontend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "start": "node --max-old-space-size=1024 dist/index.js",
    "ingest": "tsx src/cli/ingest.ts",
    "ingest:full": "tsx src/cli/ingest.ts --full",
    "install:all": "npm install && cd frontend && npm install",
    "lint": "eslint src --ext .ts",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@langchain/openai": "^0.3.0",
    "@qdrant/js-client-rest": "^1.12.0",
    "adm-zip": "^0.5.10",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.1",
    "front-matter": "^4.0.2",
    "helmet": "^8.0.0",
    "inquirer": "^9.2.12",
    "langchain": "^0.3.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.8",
    "ora": "^8.0.1",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "sql.js": "^1.12.0",
    "tiktoken": "^1.0.17",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.10.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.16.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
